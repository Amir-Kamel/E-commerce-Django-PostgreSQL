{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Ecommerce.wsgi.application"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Ecommerce.settings",
    "PYTHONUNBUFFERED": "1"
  }
}

